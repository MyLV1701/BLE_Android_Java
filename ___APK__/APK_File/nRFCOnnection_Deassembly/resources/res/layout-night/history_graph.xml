    androidPlot.graphWidget.domainOriginLabelPaint.textSize="@dimen/domain_tick_label_font_size"
    androidPlot.graphWidget.domainOriginLinePaint.color="#ffffff"
    androidPlot.graphWidget.gridBackgroundPaint.color="@color/logAndGraphBackground"
