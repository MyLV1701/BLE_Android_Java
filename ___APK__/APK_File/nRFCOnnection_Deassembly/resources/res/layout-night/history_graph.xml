<?xml version="1.0" encoding="utf-8"?>
<com.androidplot.xy.XYPlot xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/plot"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    androidPlot.backgroundPaint.color="@color/logAndGraphBackground"
    androidPlot.borderPaint.color="@color/logAndGraphBackground"
    androidPlot.domainLabel="@string/time_label"
    androidPlot.domainLabelWidget.labelPaint.color="@color/nordicBlue"
    androidPlot.domainLabelWidget.labelPaint.textSize="@dimen/range_label_font_size"
    androidPlot.graphWidget.backgroundPaint.color="@color/logAndGraphBackground"
    androidPlot.graphWidget.domainLabelPaint.color="#ffffff"
    androidPlot.graphWidget.domainLabelPaint.textSize="@dimen/domain_tick_label_font_size"
