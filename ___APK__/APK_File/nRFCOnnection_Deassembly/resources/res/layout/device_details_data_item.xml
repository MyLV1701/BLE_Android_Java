<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:background="@color/listBackgroundNormal"
    android:paddingLeft="12dp"
    android:paddingRight="6dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/bar"
        android:layout_width="5dp"
        android:layout_height="match_parent"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/ic_packet_data_indicator"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp">
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/flags"/>
        <TextView
            android:id="@+id/flags"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/no_flags"/>
        <RelativeLayout
            android:id="@+id/flags_details"
            android:paddingLeft="4dp"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/table"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="3dp"
                android:src="@drawable/flags_table"/>
            <TextView
                android:enabled="false"
                android:textSize="12sp"
                android:id="@+id/flag_limited_discoverable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="@string/flag_limited_discoverable"
                android:layout_toRightOf="@+id/table"/>
            <TextView
                android:enabled="false"
                android:textSize="12sp"
                android:id="@+id/flag_general_discoverable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/flag_general_discoverable"
                android:layout_toRightOf="@+id/table"
                android:layout_below="@+id/flag_limited_discoverable"/>
            <TextView
                android:enabled="false"
                android:textSize="12sp"
                android:id="@+id/flag_br_erd_not_supported"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/flag_br_erd_not_supported"
                android:layout_toRightOf="@+id/table"
                android:layout_below="@+id/flag_general_discoverable"/>
            <TextView
                android:enabled="false"
                android:textSize="12sp"
                android:id="@+id/flag_le_and_br_erd_controller"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/flag_le_and_br_erd_controller"
                android:layout_toRightOf="@+id/table"
                android:layout_below="@+id/flag_br_erd_not_supported"/>
            <TextView
                android:enabled="false"
                android:textSize="12sp"
                android:id="@+id/flag_le_and_br_erd_host"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/flag_le_and_br_erd_host"
                android:layout_toRightOf="@+id/table"
                android:layout_below="@+id/flag_le_and_br_erd_controller"/>
            <TextView
                android:enabled="false"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/flag_reserved"
                android:layout_toRightOf="@+id/table"
                android:layout_below="@+id/flag_le_and_br_erd_host"/>
        </RelativeLayout>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_incomplete_16_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_incomplete_16_bit"/>
        <TextView
            android:id="@+id/services_incomplete_16_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_complete_16_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_complete_16_bit"/>
        <TextView
            android:id="@+id/services_complete_16_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_incomplete_32_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_incomplete_32_bit"/>
        <TextView
            android:id="@+id/services_incomplete_32_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_complete_32_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_complete_32_bit"/>
        <TextView
            android:id="@+id/services_complete_32_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_incomplete_128_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_incomplete_128_bit"/>
        <TextView
            android:id="@+id/services_incomplete_128_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_complete_128_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_complete_128_bit"/>
        <TextView
            android:id="@+id/services_complete_128_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_solicitation_16_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_solicitation_16_bit"/>
        <TextView
            android:id="@+id/services_solicitation_16_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_solicitation_32_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_solicitation_32_bit"/>
        <TextView
            android:id="@+id/services_solicitation_32_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/services_solicitation_128_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/services_solicitation_128_bit"/>
        <TextView
            android:id="@+id/services_solicitation_128_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/service_data_16_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/service_data_16_bit"/>
        <TextView
            android:id="@+id/service_data_16_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/service_data_32_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/service_data_32_bit"/>
        <TextView
            android:id="@+id/service_data_32_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="?android:attr/textColorSecondary"
            android:id="@+id/service_data_128_bit_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/service_data_128_bit"/>
        <TextView
            android:id="@+id/service_data_128_bit"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="@color/red"
            android:id="@+id/parsing_errors_title"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/parsing_errors"/>
        <TextView
            android:id="@+id/parsing_errors"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</LinearLayout>
